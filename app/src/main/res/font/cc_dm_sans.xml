<?xml version="1.0" encoding="utf-8"?>
<font-family
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <font
        android:fontStyle="normal"
        android:fontWeight="400"
        android:font="@font/dm_sans_regular"
        app:fontStyle="normal"
        app:fontWeight="400"
        app:font="@font/dm_sans_regular" />
    <font
        android:fontStyle="italic"
        android:fontWeight="400"
        android:font="@font/dm_sans_regular_italic"
        app:fontStyle="italic"
        app:fontWeight="400"
        app:font="@font/dm_sans_regular_italic" />
    <font
        android:fontStyle="normal"
        android:fontWeight="500"
        android:font="@font/dm_sans_medium"
        app:fontStyle="normal"
        app:fontWeight="500"
        app:font="@font/dm_sans_medium" />
    <font
        android:fontStyle="italic"
        android:fontWeight="500"
        android:font="@font/dm_sans_medium_italic"
        app:fontStyle="italic"
        app:fontWeight="500"
        app:font="@font/dm_sans_medium_italic" />
    <font
        android:fontStyle="normal"
        android:fontWeight="700"
        android:font="@font/dm_sans_bold"
        app:fontStyle="normal"
        app:fontWeight="700"
        app:font="@font/dm_sans_bold" />
    <font
        android:fontStyle="italic"
        android:fontWeight="700"
        android:font="@font/dm_sans_bold_italic"
        app:fontStyle="italic"
        app:fontWeight="700"
        app:font="@font/dm_sans_bold_italic" />
</font-family>
